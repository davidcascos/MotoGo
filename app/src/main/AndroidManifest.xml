<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.dcascos.motogo">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_motogo"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_motogo_round"
		android:supportsRtl="true"
		android:theme="@style/Theme.MotoGo">
		<activity android:name=".Layouts.SplashScreen">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".Layouts.Login"></activity>
		<activity android:name=".Layouts.SignUp"></activity>

		<meta-data
			android:name="preloaded_fonts"
			android:resource="@array/preloaded_fonts" />
	</application>

</manifest>